package com.company;
import java.sql.SQLOutput;
import java.util.Scanner;
import java.lang.Math;

public class Main {

    public static void main(String[] args)
    {

       Scanner sc=new Scanner(System.in);
       float a=sc.nextFloat();
       float b=sc.nextFloat();
       float c;
       c=(int)(a*100)-(int)(b*100);
       if(c==0)
       {
           System.out.println("Yes!Both the digits are same up to the two decimal places");
       }
       else
       {
           System.out.println("They aren't same up to the decimal places");
       }
    }
}
