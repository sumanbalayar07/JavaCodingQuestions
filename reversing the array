import java.util.Arrays;
import java.util.Scanner;

public class newloop {
    public static void main(String[] args) {
        int []a={1,3,6,4,7,5};
        for(int i=0;i<a.length/2;i++)
        {
            int temp=a[i];
            a[i]=a[a.length-i-1];
            a[a.length-i-1]=temp;
        }
        System.out.println(Arrays.toString(a));    
    }
}

