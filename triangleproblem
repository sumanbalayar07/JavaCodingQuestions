public class forloop {
    public static void main(String [] args)
    {
        for(int i=1;i<=6;i++)
        {
            System.out.print(" ");
        }
        for(int i=1;i<=5;i++)
        {
            for(int j=1;j<=2*i-1;j++)
            {
                System.out.print("*");
            }
            System.out.println();
            for(int k=i;k<=5;k++)
            {
                System.out.print(" ");
            }
        }

    }
}
