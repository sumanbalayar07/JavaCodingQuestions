import java.util.*;
public class happy {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int []a={3,4,5,6,3,2,6,7,8,89};
        int n=sc.nextInt();
        Arrays.sort(a);
        System.out.println("The value of"+n+"th"+"smallest element is"+a[n-1]);
        System.out.println("The value of"+n+"th"+"largest element is"+a[a.length-n]);
    }
}
